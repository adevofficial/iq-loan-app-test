{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dest/server.js",
    "build": "babel src -d dest --copy-files",
    "clean": "rimraf ./dest",
    "build:all": "run-s clean build",
    "build:run": "run-s build:all start",
    "start:dev": "cross-env NODE_ENV=development nodemon  --exec npm run build:run",
    "db:clean": "npx sequelize-cli db:drop && npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:preload": "run-s db:migrate db:seed",
    "pretest": "cross-env NODE_ENV=test run-s db:clean db:migrate db:seed",
    "test": "jest",
    "test-docker": "jest"
  },
  "dependencies": {
    "@casl/ability": "^4.1.6",
    "@types/jest": "^26.0.15",
    "bcrypt": "^5.0.0",
    "change-case": "^4.1.1",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "~4.2.0",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "express-winston": "^4.0.5",
    "faker": "^5.1.0",
    "helmet": "^4.1.1",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.4.2",
    "pg-hstore": "^2.3.3",
    "query-string": "^6.13.7",
    "sequelice-query": "^1.0.3",
    "sequelize": "^6.3.5",
    "sequelize-paginate": "^1.1.6",
    "sift": "^13.3.5",
    "slugify": "^1.4.5",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.0.1"
  }
}
